</servlet-mapping>
  <servlet-mapping>
    <servlet-name>SavePicServlet</servlet-name>
    <url-pattern>/SavePicServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeletePicServlet</servlet-name>
