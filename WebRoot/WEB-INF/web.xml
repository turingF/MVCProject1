    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SetUserInfoServlet</servlet-name>
    <url-pattern>/SetUserInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>FindUserByAccountServlet</servlet-name>
